#include <iostream>
using namespace std;

int main() {
    int num1,num2,num3;

    cout << "Enter the first number:\n ";
    cin >> num1;

    cout << "Enter the second number:\n ";
    cin >> num2;

    cout << "Enter the third number:\n ";
    cin >> num3;

    int maximum;
    if (num1 >= num2 && num1 >= num3)
        maximum = num1;
    else if (num2 >= num1 && num2 >= num3)
        maximum = num2;
    else
        maximum = num3;

    cout << "The maximum number is: " << maximum << endl;

    return 0;
}
